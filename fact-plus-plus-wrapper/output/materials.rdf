        <emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9 xml:lang="en">Gas is a compressible fluid, a state of matter that has no fixed shape and no fixed volume.</emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
        <skos:prefLabel xml:lang="en">Gas</skos:prefLabel>
    </owl:Class>
    


    <!-- http://emmo.info/emmo#EMMO_0b15f4ae_092e_4487_9100_3c44176c545c -->


