        </owl:disjointUnionOf>
        <emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9 xml:lang="en">A material in which distributed particles of one phase are dispersed in a different continuous phase.</emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
        <skos:prefLabel xml:lang="en">Dispersion</skos:prefLabel>
    </owl:Class>
    


    <!-- http://emmo.info/emmo#EMMO_0e030040_98a7_49b2_a871_dced1f3a6131 -->


    <owl:Class rdf:about="http://emmo.info/emmo#EMMO_0e030040_98a7_49b2_a871_dced1f3a6131">
        <rdfs:subClassOf rdf:resource="http://emmo.info/emmo#EMMO_ec2c8ac8_98c5_4c74_b85b_ff8e8ca6655c"/>
        <rdfs:subClassOf>
            <owl:Restriction>
