        <rdfs:subClassOf rdf:resource="http://emmo.info/emmo#EMMO_1f5e3e7e_72c9_40d4_91dd_ae432d7b7018"/>
        <rdfs:subClassOf rdf:resource="http://emmo.info/emmo#EMMO_a2b006f2_bbfd_4dba_bcaa_3fca20cd6be1"/>
