        <rdfs:subClassOf rdf:resource="http://emmo.info/emmo#EMMO_b29fd350_39aa_4af7_9459_3faa0544cba6"/>
        <emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9 xml:lang="en">A physics-based model based on a physics equation describing the behaviour of atoms.</emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
