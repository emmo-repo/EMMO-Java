        <emmo:EMMO_705f27ae_954c_4f13_98aa_18473fc52b25 xml:lang="en">From Latin partire, partiri ‘divide, share’.</emmo:EMMO_705f27ae_954c_4f13_98aa_18473fc52b25>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://emmo.info/emmo#EMMO_4f226cf3_6d02_4d35_8566_a9e641bc6ff3"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#altLabel"/>
        <owl:annotatedTarget xml:lang="en">Role</owl:annotatedTarget>
        <emmo:EMMO_705f27ae_954c_4f13_98aa_18473fc52b25 xml:lang="en">From French rôle, from obsolete French roule ‘roll’, referring originally to the roll of paper on which the actor&apos;s part was written.</emmo:EMMO_705f27ae_954c_4f13_98aa_18473fc52b25>
    </owl:Axiom>
    


