    <!-- http://emmo.info/emmo#EMMO_494b372c_cfdf_47d3_a4de_5e037c540de8 -->


    <owl:Class rdf:about="http://emmo.info/emmo#EMMO_494b372c_cfdf_47d3_a4de_5e037c540de8">
        <rdfs:subClassOf rdf:resource="http://emmo.info/emmo#EMMO_86ca9b93_1183_4b65_81b8_c0fcd3bba5ad"/>
        <emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9 xml:lang="en">An object which is instrumental for reaching a particular purpose through its characteristic functioning process, with particular reference to mechanical or electronic equipment.</emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
        <skos:altLabel xml:lang="en">Equipment</skos:altLabel>
        <skos:altLabel xml:lang="en">Machine</skos:altLabel>
        <skos:prefLabel xml:lang="en">Device</skos:prefLabel>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://emmo.info/emmo#EMMO_494b372c_cfdf_47d3_a4de_5e037c540de8"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#altLabel"/>
        <owl:annotatedTarget xml:lang="en">Equipment</owl:annotatedTarget>
        <emmo:EMMO_705f27ae_954c_4f13_98aa_18473fc52b25 xml:lang="en">From French équipement, from équiper ‘equip’.</emmo:EMMO_705f27ae_954c_4f13_98aa_18473fc52b25>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://emmo.info/emmo#EMMO_494b372c_cfdf_47d3_a4de_5e037c540de8"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#altLabel"/>
        <owl:annotatedTarget xml:lang="en">Machine</owl:annotatedTarget>
        <emmo:EMMO_705f27ae_954c_4f13_98aa_18473fc52b25 xml:lang="en">From Latin māchina (“a machine, engine, contrivance, device, stratagem, trick”), from Doric Greek μᾱχᾰνᾱ́ (mākhanā́), cognate with Attic Greek μηχᾰνή (mēkhanḗ, “a machine, engine, contrivance, device”), from which comes mechanical.</emmo:EMMO_705f27ae_954c_4f13_98aa_18473fc52b25>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://emmo.info/emmo#EMMO_494b372c_cfdf_47d3_a4de_5e037c540de8"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#prefLabel"/>
        <owl:annotatedTarget xml:lang="en">Device</owl:annotatedTarget>
        <emmo:EMMO_705f27ae_954c_4f13_98aa_18473fc52b25 xml:lang="en">From Old French &quot;deviser&quot;, meaning: arrange, plan, contrive. Literally &quot;dispose in portions,&quot; from Vulgar Latin &quot;divisare&quot;, frequentative of Latin dividere, meaning &quot;to divide&quot;.</emmo:EMMO_705f27ae_954c_4f13_98aa_18473fc52b25>
    </owl:Axiom>
    


