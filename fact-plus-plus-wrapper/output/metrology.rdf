    <!-- http://emmo.info/emmo#EMMO_c9c8f824_9127_4f93_bc21_69fe78a7f6f2 -->


    <owl:Class rdf:about="http://emmo.info/emmo#EMMO_c9c8f824_9127_4f93_bc21_69fe78a7f6f2">
        <rdfs:subClassOf rdf:resource="http://emmo.info/emmo#EMMO_18ce5200_00f5_45bb_8c6f_6fb128cd41ae"/>
        <emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9 xml:lang="en">A reference unit provided by a measurement procedure.</emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
        <emmo:EMMO_b432d2d5_25f4_4165_99c5_5935a7763c1a xml:lang="en">Rockwell C hardness of a given sample (150 kg load): 43.5HRC(150 kg)</emmo:EMMO_b432d2d5_25f4_4165_99c5_5935a7763c1a>
        <skos:prefLabel xml:lang="en">ProcedureUnit</skos:prefLabel>
