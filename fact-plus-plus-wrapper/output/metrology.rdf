    <!-- http://emmo.info/emmo#EMMO_847724b7_acef_490e_9f0d_67da967f2812 -->


    <owl:Class rdf:about="http://emmo.info/emmo#EMMO_847724b7_acef_490e_9f0d_67da967f2812">
        <rdfs:subClassOf rdf:resource="http://emmo.info/emmo#EMMO_dd4a7f3e_ef56_466c_ac1a_d2716b5f87ec"/>
        <emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9 xml:lang="en">A non-negative parameter characterising the dispersion of the quantity being measured.</emmo:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
