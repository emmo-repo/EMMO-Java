    <!-- http://emmo.info/emmo#hasDeducer -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasDeducer">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasInterpreter"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Index"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Deducer"/>
        <skos:prefLabel xml:lang="en">hasDeducer</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasOutcome -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasOutcome">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasHolisticPart"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Process"/>
        <emmo:elucidation xml:lang="en">The relation between a process and the entity that represents how things have turned out.</emmo:elucidation>
        <skos:prefLabel xml:lang="en">hasOutcome</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasDeclared -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasDeclared">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasReferent"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Declarer"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Declared"/>
        <skos:prefLabel xml:lang="en">hasDeclared</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasStatus -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasStatus">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasHolisticTemporalPart"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Process"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Status"/>
        <skos:prefLabel xml:lang="en">hasStatus</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasPart -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasPart">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasOverlap"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <emmo:elucidation xml:lang="en">The primitive relation that express the concept of an object being part of another one.</emmo:elucidation>
        <rdfs:comment xml:lang="en">All other mereology relations are defined using hasPart as primitive. It is expressed in FOL by predicate P.</rdfs:comment>
        <skos:prefLabel xml:lang="en">hasPart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#semiotical -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#semiotical">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#EMMORelation"/>
        <emmo:elucidation xml:lang="en">The generic EMMO semiotical relation.</emmo:elucidation>
        <skos:prefLabel xml:lang="en">semiotical</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasIndex -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasIndex">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasSign"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Deduced"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Index"/>
        <skos:prefLabel xml:lang="en">hasIndex</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasInterval -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasInterval">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasRedundantPart"/>
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasTemporalPart"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Process"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Redundant"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Process"/>
        <emmo:elucidation xml:lang="en">The relation between a process whole and a temporal part of the same type.</emmo:elucidation>
        <skos:prefLabel xml:lang="en">hasInterval</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasTemporaryParticipant -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasTemporaryParticipant">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasHolisticOvercross"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Process"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Object"/>
        <rdfs:comment xml:lang="en">The relation between a process P and an object whole O that overcrosses it. The intersection between P and O is a participant of P.</rdfs:comment>
        <skos:prefLabel xml:lang="en">hasTemporaryParticipant</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasIcon -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasIcon">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasSign"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Cognised"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Icon"/>
        <skos:prefLabel xml:lang="en">hasIcon</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasComponent -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasComponent">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasConstituent"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#HolisticSystem"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Component"/>
        <skos:prefLabel xml:lang="en">hasComponent</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#mereological -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#mereological">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#EMMORelation"/>
        <emmo:elucidation xml:lang="en">The superclass of all mereological EMMO relations.</emmo:elucidation>
        <rdfs:comment xml:lang="en">The EMMO adheres to Atomistic General Extensional Mereology (AGEM).</rdfs:comment>
        <skos:prefLabel xml:lang="en">mereological</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasInterpreter -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasInterpreter">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#semiotical"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Sign"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Interpreter"/>
        <skos:prefLabel xml:lang="en">hasInterpreter</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#isTimeContactWith -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#isTimeContactWith">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#isDirectCauseFor"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <skos:altLabel xml:lang="en">isTemporalNextOf</skos:altLabel>
        <skos:prefLabel xml:lang="en">isTimeContactWith</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#isCausallyDisconnectedWith -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#isCausallyDisconnectedWith">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#topological"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Item"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Item"/>
        <skos:prefLabel xml:lang="en">isCausallyDisconnectedWith</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasCognised -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasCognised">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasReferent"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Cogniser"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Cognised"/>
        <skos:prefLabel xml:lang="en">hasCognised</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasCogniser -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasCogniser">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasInterpreter"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Icon"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Cogniser"/>
        <skos:prefLabel xml:lang="en">hasCogniser</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasHolisticOvercross -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasHolisticOvercross">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasHolisticRelation"/>
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasOvercrossing"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Whole"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Whole"/>
        <emmo:elucidation xml:lang="en">A relation between two holistic wholes that overcross, sharing one of their holistic parts.</emmo:elucidation>
        <emmo:example xml:lang="en">A man and the process of building a house. 
The man is a whole that possesses an holistic temporal part which is an interval of six monts and represents a working period in his lifetime.
The process of building a house is a whole that possesses an holistic spatial part which is a builder.
The working period of the man and the builder participating the building process are the same individual, belonging both to a man lifetime and to a building holistic views.
In this sense, the man and the building process overcrosses. and the overlapping individual is represented differently in both holistic views.</emmo:example>
        <rdfs:comment xml:lang="en">This relation is about two wholes that overlap, and whose intersection is an holistic part of both.</rdfs:comment>
        <skos:prefLabel xml:lang="en">hasHolisticOvercross</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#isDirectCauseFor -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#isDirectCauseFor">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasContactWith"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <skos:prefLabel xml:lang="en">isDirectCauseFor</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#isDirectEffectOf -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#isDirectEffectOf">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasContactWith"/>
        <owl:inverseOf rdf:resource="http://emmo.info/emmo#isDirectEffectOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <skos:prefLabel xml:lang="en">isDirectEffectOf</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasHolisticSpatialPart -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasHolisticSpatialPart">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasHolisticPart"/>
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasSpatialPart"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#SpatialRole"/>
        <skos:prefLabel xml:lang="en">hasHolisticSpatialPart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasSign -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasSign">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#semiotical"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#SemioticObject"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Sign"/>
        <skos:prefLabel xml:lang="en">hasSign</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasHolisticRelation -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasHolisticRelation">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasOverlap"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Whole"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Holistic"/>
        <emmo:elucidation xml:lang="en">The relation between a holistic whole and its related entities, being them parts or other overlapping entities.</emmo:elucidation>
        <skos:prefLabel xml:lang="en">hasHolisticRelation</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasTemporalDirectPart -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasTemporalDirectPart">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasDirectPart"/>
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasTemporalPart"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Existent"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#State"/>
        <emmo:elucidation>A relation that establishes for the whole a univocal tessellation in temporal parts.</emmo:elucidation>
        <skos:prefLabel xml:lang="en">hasTemporalDirectPart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasSpatialDirectIncidentalPart -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasSpatialDirectIncidentalPart">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasSpatialIncidentalPart"/>
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasDirectPart"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <emmo:elucidation>A relation that establishes for the whole a univocal tessellation in spatio temporal parts.</emmo:elucidation>
        <skos:prefLabel xml:lang="en">hasSpatialDirectIncidentalPart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#isCausallyconnectedWith -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#isCausallyconnectedWith">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#topological"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Item"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Item"/>
        <rdfs:comment xml:lang="en">Causality is a topological property between connected items.

Items being connected means that there is a topological contact or &quot;interaction&quot; between them.</rdfs:comment>
        <skos:prefLabel xml:lang="en">isCausallyconnectedWith</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasRedundantPart -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasRedundantPart">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasHolisticRelation"/>
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasSpatialPart"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Redundant"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Whole"/>
        <emmo:elucidation xml:lang="en">The relation between a whole and its mereological parts that are still holistic wholes.</emmo:elucidation>
        <emmo:example xml:lang="en">A volume of water has redundand parts other volumes of water. All this volumes have holistic parts some water molecules.</emmo:example>
        <rdfs:comment xml:lang="en">The purpose of this relation is to provide a parhood relation that does not go deep enough, in terms of decomposition, to break the holistic definition of the whole.

On the contrary, the holistic parthood, is expected to go that deep.</rdfs:comment>
        <skos:prefLabel xml:lang="en">hasRedundantPart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasMember -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasMember">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasProperPart"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Collection"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Item"/>
        <emmo:elucidation xml:lang="en">The relation between a collection and one of its items.</emmo:elucidation>
        <skos:prefLabel xml:lang="en">hasMember</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasSpatialIncidentalPart -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasSpatialIncidentalPart">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasSpatialPart"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <emmo:elucidation xml:lang="en">A proper part of the whole that is not Spatial or Temporal.</emmo:elucidation>
        <rdfs:comment xml:lang="en">This relation catche generic parts of a 4D object that does not fully cover the lifetime extent of the whole (spatial) nor the full spatial extent (temporal).</rdfs:comment>
        <skos:prefLabel xml:lang="en">hasSpatialIncidentalPart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasSubObject -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasSubObject">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasHolisticTemporalPart"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Object"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#SubObject"/>
        <skos:prefLabel xml:lang="en">hasSubObject</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasDirectPart -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasDirectPart">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasProperPart"/>
        <skos:altLabel xml:lang="en">isReductionisticWholeFor</skos:altLabel>
        <skos:prefLabel xml:lang="en">hasDirectPart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasTemporalPart -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasTemporalPart">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasProperPart"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#CausalObject"/>
        <emmo:elucidation xml:lang="en">A relation that identify a proper item part of the whole, whose parts always cover the full spatial extension of the whole within a time interval.</emmo:elucidation>
        <rdfs:comment xml:lang="en">In EMMO FOL this is a defined property. In OWL temporal relations are primitive.</rdfs:comment>
        <skos:prefLabel xml:lang="en">hasTemporalPart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasInterpretant -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasInterpretant">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasSign"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Interpretant"/>
        <skos:prefLabel xml:lang="en">hasInterpretant</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#isSpaceContactWith -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#isSpaceContactWith">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#isDirectCauseFor"/>
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#isDirectEffectOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <skos:prefLabel xml:lang="en">isSpaceContactWith</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasHolisticPart -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasHolisticPart">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasHolisticRelation"/>
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasProperPart"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Role"/>
        <emmo:elucidation xml:lang="en">The relation between the whole and a proper part of the whole that scale down to the point which it lose the characteristics of the whole and become something else.</emmo:elucidation>
        <emmo:example xml:lang="en">An holistic part of water fluid is a water molecule.</emmo:example>
        <skos:prefLabel xml:lang="en">hasHolisticPart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasProperPart -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasProperPart">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasPart"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <emmo:definition xml:lang="en">PPxy ≡ Pxy ∧ ¬Pyx</emmo:definition>
        <skos:prefLabel xml:lang="en">hasProperPart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasContactWith -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasContactWith">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#isCauseFor"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <skos:prefLabel xml:lang="en">hasContactWith</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasOvercrossing -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasOvercrossing">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasOverlap"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <emmo:definition xml:lang="en">OXxy ≡ Oxy ∧ ¬Pxy</emmo:definition>
        <skos:prefLabel xml:lang="en">hasOvercrossing</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasHolisticTemporalPart -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasHolisticTemporalPart">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasTemporalPart"/>
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasHolisticPart"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#TemporalRole"/>
        <skos:prefLabel xml:lang="en">hasHolisticTemporalPart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasDeduced -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasDeduced">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasReferent"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Deducer"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Deduced"/>
        <skos:prefLabel xml:lang="en">hasDeduced</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#topological -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#topological">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#EMMORelation"/>
        <emmo:elucidation xml:lang="en">The superclass of all topological EMMO relations.</emmo:elucidation>
        <skos:prefLabel xml:lang="en">topological</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasParticipant -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasParticipant">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasHolisticSpatialPart"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Process"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Participant"/>
        <emmo:elucidation xml:lang="en">The relation between a process and an object participating to it, i.e. that is relevant to the process itself.</emmo:elucidation>
        <rdfs:comment xml:lang="en">Participation is a parthood relation: you must be part of the process to contribute to it. A participant whose 4D extension is totally contained within the process.

Participation is not under direct parthood since a process is not strictly related to reductionism, but it&apos;s a way to categorize temporal regions by the interpreters.</rdfs:comment>
        <skos:prefLabel xml:lang="en">hasParticipant</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasPortion -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasPortion">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasRedundantPart"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Object"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Redundant"/>
        <rdfs:range rdf:resource="http://emmo.info/emmo#Object"/>
        <emmo:elucidation xml:lang="en">The relation between a object whole and its spatial of the same type.</emmo:elucidation>
        <emmo:example xml:lang="en">A volume of 1 cc of milk within a 1 litre can be considered still milk as a whole. If you scale down to a cluster of molecules, than the milk cannot be considered a fluid no more (and then no more a milk).</emmo:example>
        <skos:prefLabel xml:lang="en">hasPortion</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasSpatialDirectEssentialPart -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasSpatialDirectEssentialPart">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasDirectPart"/>
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasSpatialEssentialPart"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#State"/>
        <emmo:elucidation>A relation that establishes for the whole a univocal tessellation in spatial parts.</emmo:elucidation>
        <skos:prefLabel xml:lang="en">hasSpatialDirectEssentialPart</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#isIndirectCauseFor -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#isIndirectCauseFor">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#isCauseFor"/>
