    <!-- http://emmo.info/emmo#hasSymbolData -->


    <owl:DatatypeProperty rdf:about="http://emmo.info/emmo#hasSymbolData">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Symbol"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <skos:prefLabel xml:lang="en">hasSymbolData</skos:prefLabel>
    </owl:DatatypeProperty>
    


