        </rdfs:subClassOf>
        <emmo:elucidation xml:lang="en">A &apos;Sign&apos; that stands for an &apos;Object&apos; due to causal continguity.</emmo:elucidation>
        <emmo:example xml:lang="en">Smoke stands for a combustion process (a fire).
My facial expression stands for my emotional status.</emmo:example>
        <skos:prefLabel xml:lang="en">Index</skos:prefLabel>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://emmo.info/emmo#Index"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#prefLabel"/>
        <owl:annotatedTarget xml:lang="en">Index</owl:annotatedTarget>
        <emmo:etymology xml:lang="en">From Latin index (“a discoverer, informer, spy; of things, an indicator, the forefinger, a title, superscription”), from indicō (“point out, show”).</emmo:etymology>
    </owl:Axiom>
    


    <!-- http://emmo.info/emmo#TemporalRole -->


