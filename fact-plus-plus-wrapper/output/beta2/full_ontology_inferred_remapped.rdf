        <skos:prefLabel xml:lang="en">hasOverlap</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://emmo.info/emmo#hasConstituent -->


    <owl:ObjectProperty rdf:about="http://emmo.info/emmo#hasConstituent">
        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasHolisticSpatialPart"/>
