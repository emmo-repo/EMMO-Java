        <rdfs:subPropertyOf rdf:resource="http://emmo.info/emmo#hasHolisticOvercross"/>
        <rdfs:domain rdf:resource="http://emmo.info/emmo#Process"/>
        <emmo:elucidation xml:lang="en">The outcome of an intentional process.</emmo:elucidation>
        <rdfs:comment xml:lang="en">The partial overlapping is required since the creating process is distinct with the process in which the product is used or consumed.</rdfs:comment>
        <skos:altLabel xml:lang="en">hasProduct</skos:altLabel>
        <skos:prefLabel xml:lang="en">hasProcessOutput</skos:prefLabel>
    </owl:ObjectProperty>
    


