        <rdfs:range rdf:resource="http://emmo.info/emmo#Constituent"/>
        <emmo:elucidation xml:lang="en">The relation between an object and one of its holistic part that contributes to the object under some spatial-based criteria.</emmo:elucidation>
        <skos:prefLabel xml:lang="en">hasConstituent</skos:prefLabel>
    </owl:ObjectProperty>
    


